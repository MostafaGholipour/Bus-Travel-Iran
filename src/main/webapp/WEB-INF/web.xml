<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">
    <servlet>
        <servlet-name>Login</servlet-name>
        <servlet-class>com.example.hw23.controller.LoginServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Register</servlet-name>
        <servlet-class>com.example.hw23.controller.RegisterServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>BusResult</servlet-name>
        <servlet-class>com.example.hw23.controller.BusServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Ticket</servlet-name>
        <servlet-class>com.example.hw23.controller.TicketServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Buy</servlet-name>
        <servlet-class>com.example.hw23.controller.BuyServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>MyTicketServlet</servlet-name>
        <servlet-class>com.example.hw23.controller.MyTicketServlet</servlet-class>
    </servlet>

    <servlet>
        <servlet-name>ShowTicket</servlet-name>
        <servlet-class>com.example.hw23.controller.ShowTicket</servlet-class>
    </servlet>




    <servlet-mapping>
        <servlet-name>Login</servlet-name>
        <url-pattern>/Login</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Register</servlet-name>
        <url-pattern>/register</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>BusResult</servlet-name>
        <url-pattern>/BusResult</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Ticket</servlet-name>
        <url-pattern>/Ticket</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Buy</servlet-name>
        <url-pattern>/Buy</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>MyTicketServlet</servlet-name>
        <url-pattern>/MyTicketServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ShowTicket</servlet-name>
        <url-pattern>/ShowTicket</url-pattern>
    </servlet-mapping>


</web-app>